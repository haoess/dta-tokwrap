<?xml version="1.0"?>
<TEI xmlns="http://www.tei-c.org/ns/1.0">
  <text>
    <body>
      <div n="simple-note">
	<seg part="I"><note n="simple">simple:I</note></seg>
	<pb/>
	<seg part="F"><note n="simple">simple:F</note></seg>
      </div>
      <div n="long-note">
	<seg part="I"><note n="long">long:I</note></seg>
	<pb/>
	<seg part="M"><note n="long">long:M</note></seg>
	<pb/>
	<seg part="M"><note n="long">long:M</note></seg>
	<pb/>
	<seg part="F"><note n="long">long:F</note></seg>
      </div>
      <div n="cross">
	<seg part="I"><note n="cross1">cross1:I</note></seg>
	<seg part="I"><note n="cross2">cross2:I</note></seg>
	<pb/>
	<seg part="F"><note n="cross1">cross1:F</note></seg>
	<seg part="F"><note n="cross2">cross2:F</note></seg>
      </div>
      <div n="long-cross:FAILS">
	<seg part="I"><note n="long-cross1">cross1:I</note></seg>
	<seg part="I"><note n="long-cross2">cross2:I</note></seg>
	<pb/>
	<seg part="M"><note n="long-cross1">cross1:M</note></seg>
	<seg part="M"><note n="long-cross2">cross2:M</note></seg>
	<pb/>
	<seg part="F"><note n="long-cross1">cross1:F</note></seg>
	<seg part="F"><note n="long-cross2">cross2:F</note></seg>
      </div>
      <div n="seg-in-note:IGNORED">
	<note n="simple"><seg part="I">seg-in-note:I</seg></note>
	<pb/>
	<note n="simple"><seg part="F">seg-in-note:F</seg></note>
      </div>
      <div n="cell:IGNORED">
	<table>
	  <seg part="I"><cell>cell:1</cell></seg>
	  <pb/>
	  <seg part="F"><cell>cell:2</cell></seg>
	</table>
      </div>
    </body>
  </text>
</TEI>
